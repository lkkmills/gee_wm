// ECUADOR

// SET-UP

// Function to assign each boundary its corresponding project ID
var setGEFID = function(feature) {
  return feature.set('GEFID', project_id);
};

// Define the project ID
var project_id = 5458

var buffers = ee.FeatureCollection('users/lkmills/proj_5458_georeference2');
print(buffers);

Map.addLayer(buffers, {color: 'blue'}, 'buffers');

var buffers_5458 = buffers.map(setGEFID);
print('buffers: ', buffers_5458);

Export.table.toDrive({
  collection: buffers_5458,
  description:'buffers_5458',
  fileFormat: 'GeoJSON'
});