var stats17_func = function(feature) {
  var lossImage = gfc2014.select(['lossyear']).eq(17);
  var stats = lossImage.reduceRegion({
    reducer: ee.Reducer.sum(),
    geometry: feature.geometry(),
    scale: 30,
    maxPixels: 1e30
